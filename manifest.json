{
    "name": "Dev-Motivator",
    "version": "1.0",
    "description": "A description of your Angular-powered Chrome extension.",
    "manifest_version": 3,
    "chrome_url_overrides": {
    "newtab": "index.html"
   },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
  }